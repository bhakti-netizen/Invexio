    <validationRules>
        <fullName>Refunded</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(
ISPICKVAL(  Repayment_Method__c , &quot;Account Credit&quot;),
ISPICKVAL(  Repayment_Method__c , &quot;Refund&quot;)
),
ISPICKVAL( Status__c , &quot;Refunded&quot;),
ISBLANK( Amount_Refunded__c )
)</errorConditionFormula>
        <errorDisplayField>Amount_Refunded__c</errorDisplayField>
        <errorMessage>YOU NEED TO FILL OUT AMOUNT REFUNDED/CREDITED</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Returned</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL( Status__c , &quot;Returned&quot;),
ISBLANK( Returned_By__c )
)</errorConditionFormula>
        <errorDisplayField>Returned_By__c</errorDisplayField>
        <errorMessage>YOU NEED TO FILL OUT WHO RETURNED THIS PRODUCT</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Shipped</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL( Status__c , &quot;Shipped&quot;),
ISBLANK( Tracking_Number__c )
)</errorConditionFormula>
        <errorDisplayField>Tracking_Number__c</errorDisplayField>
        <errorMessage>YOU NEED TO FILL OUT TRACKING INFORMATION</errorMessage>
    </validationRules>
